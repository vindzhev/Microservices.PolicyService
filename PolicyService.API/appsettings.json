{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PolicyServiceConnection": "THIS IS SECRET"
  },
  "PricingServiceUri": "http://PricingService/api/pricing",
  "spring": {
    "application": { "name": "PolicyService" },
    "cloud": {
      "config": {
        "uri": "http://localhost:5004",
        "validate_certificates": false
      }
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://localhost:8761/eureka",
      "ValidateCertificates": false
    },
    "instance": {
      "appName": "PolicyService",
      "port": "5004",
      "hostName": "localhost",
      "healthCheckUrlPath": "/health",
      "statusPageUrlPath": "/health",
      "instanceId": "localhost:5004"
    }
  },
  "RabbitMQ": {
    "Username": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "Port": 5672,
    "Hostname": "localhost",
    "RequestTimeout": "00:00:10",
    "PublishConfirmTimeout": "00:00:01",
    "RecoveryInterval": "00:00:01",
    "PersistentDeliveryMode": true,
    "AutoCloseConnection": true,
    "AutomaticRecovery": true,
    "TopologyRecovery": true,
    "Exchange": {
      "Name": "microservices-poc",
      "Durable": true,
      "AutoDelete": false,
      "Type": "Topic"
    },
    "Queue": {
      "Prefix": "payments-service-",
      "AutoDelete": false,
      "Durable": true,
      "Exclusive": false
    }
  }
}
